{
  "name": "open_brewery_db",
  "version": "1.0.0",
  "description": "Open Brewery DB App write in angularjs",
  "scripts": {
    "server:test": "node scripts/test.js server --passWithNoTests",
    "server:test:watch": "yarn run server:test --watch",
    "server:test:staged": "yarn run test -- --findRelatedTests",
    "preserver:start": "tsc -p ./server/tsconfig.json",
    "server:start": "tsc -p ./server/tsconfig.json -w & nodemon ./server/dist/index.js",
    "test:ci": "yarn run test -- --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "ts-jest": "^27.0.1",
    "ts-node": "~10.0.0",
    "typescript": "^4.3.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
